		{
			"name": "postAuthorization",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Generate random email address\r",
							"const randomEmail = \"user\" + Math.floor(Math.random() * 1000) + \"@example.com\";\r",
							"\r",
							"// Setel nilai yang dihasilkan ke dalam lingkungan Postman\r",
							"pm.environment.set(\"email_random\", randomEmail);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.environment.set(\"accessToken\", jsonData.accessToken);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Response time is less than 200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"pm.test(\"Response has text content\", function () {\r",
							"    pm.expect(pm.response.text()).to.not.be.empty;\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n   \"clientName\": \"Rifki\",\r\n   \"clientEmail\": \"{{email_random}}\"\r\n}",
